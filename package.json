{
  "name": "@itentialopensource/adapter-salesforce",
  "version": "0.2.0",
  "description": "This adapter integrates with system Salesforce",
  "main": "adapter.js",
  "wizardVersion": "2.28.6",
  "engineVersion": "1.24.1",
  "scripts": {
    "preinstall": "node utils/setup.js",
    "lint": "eslint . --ext .json --ext .js",
    "test:unit": "mocha test/unit/adapterTestUnit.js --LOG=error",
    "test:integration": "mocha test/integration/adapterTestIntegration.js --LOG=error",
    "test:cover": "nyc --reporter html --reporter text mocha --reporter dot test/*",
    "test": "npm run test:unit && npm run test:integration",
    "deploy": "npm publish --registry=http://registry.npmjs.org --access=public",
    "build": "npm run deploy",
    "artifactize": "npm i && node utils/packModificationScript.js"
  },
  "keywords": [
    "Itential",
    "IAP",
    "Automation",
    "Integration",
    "App-Artifacts",
    "Adapter",
    "ITSM",
    "Testing",
    "Salesforce",
    "Pre-Release"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:itentialopensource/adapters/adapter-salesforce.git"
  },
  "author": "Itential",
  "homepage": "https://gitlab.com:itentialopensource/adapters/adapter-salesforce#readme",
  "dependencies": {
    "@itentialopensource/adapter-utils": "^4.8.4",
    "ajv": "^6.10.0",
    "fs-extra": "^7.0.0",
    "readline-sync": "^1.4.9",
    "xmldom": "^0.1.27",
    "package-json-validator": "^0.6.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json": "^1.4.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "testdouble": "^3.8.1",
    "winston": "^2.4.0"
  },
  "private": false
}
